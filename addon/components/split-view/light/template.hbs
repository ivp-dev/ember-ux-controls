<div 
  class={{this.classNames}}
  {{split-view-modifier
    host=this
    minPaneSizes=@minPaneSizes
    sizes=@sizes
    onSizeChanged=@onSizeChanged
    axis=this.axis
    classNamesBuilder=this.classNamesBuilder
    barSize=this.barSize
    fluent=this.fluent
    sizeTarget=this.sizeTarget
    maxSizeTarget=this.maxSizeTarget
    minPaneSize=this.minPaneSize
    responsive=this.responsive
    sideOrigin=this.sideOrigin
    sideTarget=this.sideTarget
  }}
  ...attributes
  >
  {{yield (hash Pane=(
    component "split-view/pane"
    content=@content
    classNamesBuilder=this.classNamesBuilder))
  }}
</div>