<div 
  class="{{this.classNames}}"
  {{did-insert this.didInsert}}
  {{did-update this.didUpdateAttrs @barSize @responsive @minSize @minSizes}}
>
  {{#unless this.hasItemsSource}}
    {{yield (hash Column=(
      component this.itemTemplateName 
      axis=this.axis
      barSize=this.barSize
      visualParent=this
      classNamesBuilder=this.classNamesBuilder))
    }}
  {{/unless}}
</div>